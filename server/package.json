{
  "name": "raise-server",
  "version": "1.0.0",
  "main": "serverless.ts",
  "scripts": {
    "postinstall": "cp -n env.local.ts env.ts && serverless dynamodb install",
    "start": "npm run dev",
    "dev": "IS_OFFLINE=true serverless offline start",
    "build": "serverless package",
    "deploy:dev": "serverless deploy --stage dev --verbose",
    "teardown:dev": "serverless remove --stage dev --verbose",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "lint": "eslint 'src/**/*.ts' 'serverless.ts'"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.34.0",
    "@aws-sdk/lib-dynamodb": "^3.34.0",
    "@middy/core": "^2.5.1",
    "@middy/http-json-body-parser": "^2.5.1",
    "@middy/validator": "^2.5.1",
    "ajv": "^8.6.3",
    "google-auth-library": "^7.10.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "middy-middleware-jwt-auth": "^4.1.0",
    "serverless-dynamodb-local": "^0.2.40",
    "source-map-support": "^0.5.20",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.61.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/http-errors": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "aws-lambda": "^1.0.6",
    "eslint": "^7.32.0",
    "eslint-config-blvd": "^1.2.1",
    "json-schema-to-ts": "^1.6.4",
    "serverless": "^2.61.0",
    "serverless-offline": "^8.2.0",
    "serverless-webpack": "^5.5.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.57.1",
    "webpack-node-externals": "^3.0.0"
  }
}
